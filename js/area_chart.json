{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 800,
  "height": 600,
  "title": "Number of Nuclear Power Plant Construction all over the world every year",
  "data": {"url": "https://raw.githubusercontent.com/ting1999/FIT3179_Assignment2/main/data/nuclear_power_plants.csv"},
  "transform": [
    {"filter": "datum['ConstructionStartAt']"}
  ],
  "mark": "area",
  "encoding": {
    "x": {
      "timeUnit": "year", "field": "ConstructionStartAt",
      "axis": {"format": "%Y"}
    },
    "y": {
      "aggregate": "count", "field": "ConstructionStartAt",
      "title": "Number of Nuclear Power Plants"
    }
  }
}
